{
  "name": "porfolio_2023",
  "version": "1.0.0",
  "description": "My new porfolio for 2023. Build with tailwind and alpine js",
  "main": "index.js",
  "scripts": {
    "build-tw": "tailwindcss -i ./src/tailwind/input.css -o ./dist/css/main.css",
    "build-sass": "sass --no-source-map ./src/scss/main.scss ./dist/css/customStyles.css",
    "watch-tw": "tailwindcss -i ./src/tailwind/input.css -o ./dist/css/main.css --watch",
    "watch-sass": "sass --watch ./src/scss/main.scss ./dist/css/customStyle.css",
    "bundle-script": "esbuild src/alpine/script.js --outfile=dist/js/alpine.js --bundle --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shane52lucian/porfolio_2023.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shane52lucian/porfolio_2023/issues"
  },
  "homepage": "https://github.com/shane52lucian/porfolio_2023#readme",
  "devDependencies": {
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "alpinejs": "^3.12.0",
    "esbuild": "^0.17.18",
    "font-awesome": "^4.7.0"
  }
}
